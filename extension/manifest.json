{
  "manifest_version": 2,

  "name": "vdv",
  "description": "This extension shows the vector drawable when you open it",
  "version" : "0.0.1",
  "author": "Jumpei Matsuda @jmatsu",

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["bin/main.js"],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "*://*/*",
    "<all_urls>"
  ]
}
