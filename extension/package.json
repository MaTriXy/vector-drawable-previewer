{
  "name": "vdv",
  "description": "A Chrome Extension can show the vector drawable when you open it",
  "version": "1.0.0",
  "repository": "https://github.com/jmatsu/vdv",
  "private": true,
  "scripts": {
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "clean": "find src -name \"*.js\" -print0|xargs -0 -I{} rm {}; find src -name \"*.js.map\" -print0|xargs -0 -I{} rm {}",
    "dry-clean": "find src -name \"*.js\" -print0|xargs -0; find src -name \"*.js.map\" -print0|xargs -0",
    "browserify": "mkdir bin; node_modules/.bin/browserify src/main.js > bin/main.js"
  },
  "dependencies": {
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.3",
    "browserify": "13.0.1"
  },
  "devDependencies": {
    "typescript": "^2.1.4"
  }
}
